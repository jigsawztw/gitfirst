import java.io.*;

public class Solution {
         public static void main(String[] args) throws Exception {
            int x=1,y=1;
            while(y<=10){
                while(x<=10){
                    System.out.print(x*y+" ");
                    x++;
                }
                y++;
                System.out.println();
                x=1;
            }

        }
    }
