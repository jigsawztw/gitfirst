/* 
Деление на ноль
*/

public class Solution {

    public static void main(String[] args) {
        try{
            divisionByZero();
        }
        catch (ArithmeticException e){
            e.printStackTrace();
        }
    }
    public static void divisionByZero(){
        int a = 45/0;
        System.out.println(a);
    }
}
