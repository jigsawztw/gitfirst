
/* 
ООП - Перегрузка
*/

public class Solution {
    public static void main(String[] args) {
        printMatrix(2, 3, "8");
    }

    public static void printMatrix(double m, double n, String value) {
        System.out.println("Заполняем объектами String :" +n+m+value);
       
    }
    public static void printMatrix(double m, int n, String value) {
        System.out.println("Заполняем объектами String");
        
    }
    public static void printMatrix(Object m, int n, String value) {
        System.out.println("Заполняем объектами String");
        
    }
    public static void printMatrix(Object m, float n, String value) {
        System.out.println("Заполняем объектами String");

    }
    public static void printMatrix(String m, Float n, String value) {
        System.out.println("Заполняем объектами String");
      
    }
    public static void printMatrix(short m, int n, String value) {
        System.out.println("Заполняем объектами String");
        
    }
    public static void printMatrix(short m, double n, String value) {
        System.out.println("Заполняем объектами String");
       
    }
    public static void printMatrix(float m, double n, String value) {
        System.out.println("Заполняем объектами String");
        
    }
    public static void printMatrix(float m, int n, String value) {
        System.out.println("Заполняем объектами String");
      
    }
    

    public static void printMatrix(int m, int n, String value) {
        for (int i = 0; i < m; i++) {
            for (int j = 0; j < n; j++) {
                System.out.print(value);
            }
            System.out.println();
        }
    }
}
